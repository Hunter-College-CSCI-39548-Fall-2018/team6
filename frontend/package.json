{
  "name": "travelapp",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^0.18.0",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.22.2",
    "npm": "^6.4.1",
    "rc-slider": "^8.6.3",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-compound-slider": "^0.16.3",
    "react-day-picker": "^7.2.4",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "react-select": "^2.1.2",
    "react-tabs": "^2.3.0",
    "save": "^2.3.3"
  },
  "scripts": {
    "build:css": "postcss src/styles/index.css -o src/styles/main.css",
    "watch:css": "postcss src/styles/index.css -o src/styles/main.css -w",
    "dev": "yarn watch:css & react-scripts start",
    "start": "react-scripts start",
    "build": "yarn build:css && react-scripts build && rm -rf ../../src/main/resources/static/ && mv build/ ../src/main/resources/static/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "postcss-cli": "^6.0.1",
    "tailwindcss": "^0.7.2"
  }
}
